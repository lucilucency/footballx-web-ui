{
  "name": "footballx-web",
  "description": "FootballX Web UI",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "sh -ac '. ./.env.${REACT_APP_ENV}; HTTPS=${REACT_APP_HTTPS} PORT=${REACT_APP_PORT} react-scripts start'",
    "build": "npm install && npm run lintnofix && npm run rsbuild",
    "rsbuild": "sh -ac '. ./.env.${REACT_APP_ENV}; HTTPS=${REACT_APP_HTTPS} PORT=${REACT_APP_PORT} react-scripts build'",
    "snapshot": "react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.* --html",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lintnofix": "eslint . --ext .js,.jsx",
    "update:emoticons": "npm r dota2-emoticons && npm i dota2-emoticons && node dev/updateEmoticons.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "homepage": ".",
  "greenkeeper": {
    "label": "type: greenkeeper"
  },
  "repository": {
    "type": "git",
    "url": "git@code.ttab.me:web/fx-web-ui.git"
  },
  "dependencies": {
    "abcolor": "^0.5.5",
    "core-js": "^2.5.2",
    "dota2-emoticons": "^1.0.1",
    "dotaconstants": "^5.28.0",
    "form-data": "^2.3.2",
    "form-urlencoded": "^2.0.4",
    "fuzzy": "^0.1.3",
    "fxconstants": "^1.1.11",
    "heatmap.js": "^2.0.5",
    "history": "^4.7.2",
    "json2csv": "^3.11.5",
    "lodash": "^4.17.4",
    "long": "^4.0.0",
    "material-ui": "^0.20.0",
    "material-ui-next": "^1.0.0-beta.44",
    "moment": "^2.22.1",
    "prop-types": "^15.6.0",
    "react": "^16.3.1",
    "react-addons-update": "^15.6.2",
    "react-dom": "^16.3.1",
    "react-facebook": "^5.0.3",
    "react-facebook-login": "^4.0.1",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-lifecycles-compat": "^3.0.2",
    "react-markdown": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "react-style-proptype": "^3.1.0",
    "react-tooltip": "^3.4.0",
    "recharts": "^1.0.0-beta.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-responsive": "^4.3.5",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.2",
    "styled-components": "^3.1.6",
    "superagent": "^3.8.2",
    "uuid": "^3.1.0",
    "wordcloud": "^1.0.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.1.0",
    "react-snap": "^1.11.5",
    "simple-vdf": "^1.1.0"
  }
}
