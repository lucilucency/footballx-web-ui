{
  "name": "footballx-web",
  "description": "FootballX Web UI",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "sh -ac '. ./.env; HTTPS=${REACT_APP_HTTPS} PORT=${REACT_APP_PORT} react-scripts start'",
    "build": "npm install && npm run lintnofix && npm run update:fxconstants && npm run rsbuild",
    "rsbuild": "sh -ac '. ./.env; HTTPS=${REACT_APP_HTTPS} PORT=${REACT_APP_PORT} react-scripts build'",
    "snapshot": "react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.* --html",
    "lint": "eslint . --ext .js,.jsx --fix",
    "lintnofix": "eslint . --ext .js,.jsx",
    "update:emoticons": "npm r dota2-emoticons && npm i dota2-emoticons && node dev/updateEmoticons.js",
    "update:fxconstants": "sh -ac '. ./.env; REACT_APP_API_HOST=${REACT_APP_API_HOST} REACT_APP_API_CMS=${REACT_APP_API_CMS} node dev/updateFxconstants.js && node dev/updateLocalData.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "greenkeeper": {
    "label": "type: greenkeeper"
  },
  "repository": {
    "type": "git",
    "url": "git@code.ttab.me:web/fx-web-ui.git"
  },
  "dependencies": {
    "abcolor": "^0.5.5",
    "core-js": "^2.5.7",
    "dota2-emoticons": "^1.0.1",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "draftjs-to-markdown": "^0.5.1",
    "form-data": "^2.3.2",
    "form-urlencoded": "^2.0.7",
    "froala-editor": "^2.8.1",
    "fuzzy": "^0.1.3",
    "fxconstants": "^1.1.16",
    "heatmap.js": "^2.0.5",
    "history": "^4.7.2",
    "html-to-draftjs": "^1.4.0",
    "json2csv": "^3.11.5",
    "lodash": "^4.17.10",
    "long": "^4.0.0",
    "markdown-it": "^8.4.1",
    "material-ui": "^0.20.1",
    "material-ui-next": "^1.0.0-beta.47",
    "moment": "^2.22.2",
    "opengraph-io": "^1.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-addons-update": "^15.6.2",
    "react-amplitude": "^0.1.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-draft-wysiwyg": "^1.12.13",
    "react-facebook": "^5.0.3",
    "react-facebook-login-component": "^0.9.2",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-infinite": "^0.13.0",
    "react-lazyload": "^2.3.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-style-proptype": "^3.2.1",
    "react-tooltip": "^3.6.0",
    "react-transition-group": "^2.3.1",
    "recharts": "^1.0.0-beta.10",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-responsive": "^4.3.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.3",
    "styled-components": "^3.3.2",
    "superagent": "^3.8.3",
    "uuid": "^3.2.1",
    "wordcloud": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gh-pages": "^1.2.0",
    "react-snap": "^1.12.2",
    "simple-vdf": "^1.1.1"
  }
}
